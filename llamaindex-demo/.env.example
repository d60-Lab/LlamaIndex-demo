# Ollama 配置 (推荐 - 本地运行)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2  # 可选: llama3.2, qwen2.5, mistral, codellama 等
OLLAMA_EMBEDDING_MODEL=nomic-embed-text  # Ollama 嵌入模型

# Poe.com API 配置 (可选)
POE_API_KEY=your-poe-api-key-here
POE_BASE_URL=https://api.poe.com

# DeepSeek API 配置 (可选)
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# OpenAI API 配置 (可选)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Anthropic API 配置 (可选)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# Cohere API 配置 (可选)
COHERE_API_KEY=your-cohere-api-key-here

# Pinecone 配置 (可选)
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENVIRONMENT=us-west1-gcp

# LangSmith 追踪 (可选)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langsmith-api-key-here

# 模型选择配置
LLM_PROVIDER=ollama  # 可选: ollama, poe, deepseek, openai
EMBEDDING_PROVIDER=ollama  # 可选: ollama, openai

# Poe.com 模型配置 (如果使用 Poe)
POE_MODEL=claude-3-haiku-20240307  # 可选: claude-3-haiku, claude-3-sonnet, gpt-4o, gemini-pro 等